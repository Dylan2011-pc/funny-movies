{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
        "headers": {
          "host": "funny-movies-server.vercel.app",
          "origin": "https://huongandi.vercel.app",
          "referer": "https://huongandi.vercel.app/",
          "cache-control": "no-cache",
          "access-control-request-method": "GET, POST, PUT, DELETE",
          "access-control-request-headers": "Content-Type, Authorization, X-Requested-With, Accept"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/funny-api/:any*",
        "destination": "/funny-api/:any*",
        "has": [
          {
            "type": "header",
            "key": "origin",
            "value": "https://huongandi.vercel.app/"
          }
        ]
      }
    ]
  }